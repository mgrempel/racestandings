<h1><%= @season.year %></h1>
<p><a href="<%= @season.url %>">Season wikipedia link</a></p>

<% @season.circuits.each do |circuit| %>
  <h3><%= circuit.name %></h3>
    <ul>
    <% circuit.laptimes.where(season: @season).each do |laptime| %>
      <li><%= laptime.driver.first_name %> <%= laptime.driver.last_name %> with a time of <%= laptime.laptime %></li>
    <% end %>
  </ul>
<% end %>